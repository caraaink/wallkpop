{
  "builds": [
    {
      "src": "app/**/*.{js,ts,jsx,tsx}",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { "src": "/panel", "dest": "/panel" },
    { "src": "/track/([^/]+)/([^/]+)", "dest": "/track/[id]/[permalink]" },
    { "src": "/search", "dest": "/search" },
    { "src": "/api/post", "dest": "/api/post" },
    { "src": "/api/hits", "dest": "/api/hits" }
  ]
}
